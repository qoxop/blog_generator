<article class="category_list">
    <section class="category_links">
        <h2>Categoriesï¼š</h2>
        <ul>
        {{each cList}}
            <li><a href="#{{$value.name}}">{{$value.name}}</a></li>
        {{/each}}
        </ul>
    </section>
    {{each cList}}
    <section class="category_titles">
        <h3 id="{{$value.name}}">{{$value.name}}</h3>
        <ul>
        {{each $value.list essay key}}
            <li>
                <span class="essay_time">{{essay.modifyAt}}</span>
                <a href="{{essay.url}}">{{essay.title}}</a>
            </li>
        {{/each}}
        </ul>
    </section>
    {{/each}}
</article>